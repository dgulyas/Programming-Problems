 /*
10110 - Light, more light
http://uva.onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=1051
*/

#include <iostream>
#include <math.h>

using namespace std;

int main () {
  double light, intPart;
	
	cin >> light;
	while(light != 0){
		if(modf(sqrt(light),&intPart) == 0)
			cout << "yes" << endl;
		else
			cout << "no" << endl;
		cin >> light;
	}
}

